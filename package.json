{
  "name": "scripture-notes",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "npm run dev -w client",
    "dev:server": "npm run dev -w server",
    "build": "npm run build -w client",
    "db:init": "npm run db:init -w server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
